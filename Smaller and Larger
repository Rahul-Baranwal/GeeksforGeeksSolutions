class Solution {
    int[] getMoreAndLess(int[] arr, int x) {
        // code here
          int small=0, larg=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]<=x)
                small++;
            if(arr[i]>=x)
                larg++;
        }
        
        return new int[]{small,larg};
    }
}
